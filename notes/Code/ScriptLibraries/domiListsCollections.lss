%REM
	Function logJSONValue
	Converts a value to JSON and writes it to the log.
	
	@param source 			value to write to the log
	@param excludeNodes 	Names of JSON nodes to exclude from writing to the log
	@param prefix 			prefix (such as a json tag name) to be prepended to the json value
	@param depth 			number of node into the jason from the outermost level.
	@param appendComma  	Flag indicating if a comma should be appended 
	
%END REM
Private Sub logJSONValue(source As Variant, excludeNodes As BooleanList, prefix$, depth%, appendComma As Boolean) 
	On Error GoTo ErrorTrap
	
	Const INDENT = |  |
	
	Dim bl As BooleanList 

	Dim chunks As Variant 
	Dim elements As Variant 

	Dim extendedinfo List As String 
	Dim msg$

	Dim sourcetype$
	Dim lstr$
	Dim nodename$ 
	Dim result$

	Dim indents$ 
	Dim comma$ 
	Dim lastchar$ 

	Dim idx%
	Dim upperbound% 
	
	
	' BEGIN CODE
	If (excludeNodes Is Nothing) Then 
		Set bl = New BooleanList() 
		Call logJSONValue(source, bl, prefix$, depth%, appendComma)
		GoTo ExitPoint
	End If 
	
	sourcetype$ = TypeName(source) 
	extendedinfo(|SourceType|) = sourcetype$ 
	If depth% > 0 Then indents$ = String$(depth%, INDENT) 
	If appendComma Then comma$ = |,|

	
	
	' TODO: Add additional case options (as appropriate) for standard Notes product objects (Document, ViewEntry, Item, etc). 
	Select Case LCase(sourcetype$) 
		Case |boolean|:
			If (source) Then 
				enhLogExtendedContent ||, ||, indents$ & prefix$ & |true| & comma$
			Else 
				enhLogExtendedContent ||, ||, indents$ & prefix$ & |false| & comma$
			End If

		Case |date|, |time|
			enhLogExtendedContent ||, ||, indents$ & prefix$ & |"| & CStr(source) & |"| & comma$ 

		Case |azlwlistcarrier|:
			enhLogExtendedContent ||, ||, indents$ & prefix$ & |{|
			logJSONvalue source.getContent(), excludeNodes, ||, depth% + 1, appendComma	
			enhLogExtendedContent ||, ||, indents$ & |}| & comma$
			
		Case |booleanlist|, _ 
		|currencylist|, _ 
		|datelist|, _ 
		|doublelist|, _ 
		|integerlist|, _ 
		|longlist|, _ 
		|singlelist|, _ 
		|stringlist|, _ 
		|variantlist|: 
			enhLogExtendedContent ||, ||, indents$ & prefix$ & |{|
			logJSONvalue source.content, excludeNodes, ||, depth% + 1, appendComma		
			enhLogExtendedContent ||, ||, indents$ & |}| & comma$
			
		Case Else: 
			If IsArray(source) Then 
				enhLogExtendedContent ||, ||, indents$ & prefix$ & |[|
				
				For idx% = LBound(source) To UBound(source)
					If (idx% < UBound(source)) Then 
						logJSONvalue source(idx%), excludeNodes, ||, depth% + 1, True
					Else 
						logJSONvalue source(idx%), excludeNodes, ||, depth% + 1, False
					End If 
				Next

				enhLogExtendedContent ||, ||, indents$ & |]| & comma$
				
			ElseIf IsList(source) Then 
				upperbound% = -1
				ForAll element In source
					upperbound% = upperbound% + 1
				End ForAll
				If (upperbound% > -1) Then 
					idx% = 0
					If Not IsBlankString(prefix$) Then enhLogExtendedContent ||, ||, indents$ & prefix$
					ForAll element In source
						nodename$ = ListTag(element)
						
						If excludeNodes.isTrue(nodename$) Then
							If (idx% < upperbound%) Then 
								enhLogExtendedContent ||, ||, indents$ & INDENT & |"| & ampersandEncode(nodename$) & |":"**********",|
							Else 
								enhLogExtendedContent ||, ||, indents$ & INDENT & |"| & ampersandEncode(nodename$) & |":"**********"|
							End If 									 
						Else 
							If (idx% < upperbound%) Then 
								logJSONvalue element, excludeNodes, |"| & ampersandEncode(nodename$) & |":|, depth% + 1, True
							Else 
								logJSONvalue element, excludeNodes, |"| & ampersandEncode(nodename$) & |":|, depth% + 1, False
							End If 								
						End If
						
						idx% = idx% + 1
					End ForAll				
				End If 


			ElseIf IsObject(source) Then
				' General Object conversion not yet completed  
				enhLogWarning LIB_PREFIX, |Object Code not yet completed for | & sourcetype$ 
				enhLogExtendedContent ||, ||, indents$ & prefix$ & |{}| & comma$
				
			Else 
				' replace carriage returns and line feeds
				result$ = replaceCRLF(CStr(source), |\n|)  
				
				If IsNumeric(source) Then 
					result$ = LCase$(result$) ' Converts Boolean True to "true" and False to "false" 
					If (source < 1) Then 
						If ("" = result$) Or (0 = StrCompare(".", Left$(result$, 1), 5)) Then result$ = "0" & result$
					End If 
				Else 
					lstr$ = LCase(result$) 
					Select Case lstr$
						Case |true|, |false|: 
							result$ = lstr$  ' string representations of boolean value
							
						Case Else: 
							elements = Split(result$, |;|) 
							ForAll element In elements 
								chunks = Split(CStr(element), |=|)
								If (UBound(chunks) > 0) Then 
									If excludeNodes.isTrue(FullTrim(CStr(chunks(0)))) Then 
										chunks(UBound(chunks)) = |**********|
										element = Join(chunks, |=|) 
									End If
								End If
							End ForAll
							result$ = Join(elements, |;|)							
							result$ = |"| & ampersandEncode(result$) & |"|
					End Select
				End If
				
				enhLogExtendedContent ||, ||, indents$ & prefix$ & result$ & comma$
			End If	
	End Select
	
	
ExitPoint:		
	Exit Sub 
ErrorTrap:
	On Error GoTo 0
	ForAll s In extendedinfo 
		If isBlankString(msg$) Then 
			msg$ = ListTag(s) & |: | & s
		Else 
			msg$ = msg$ & Chr(10) & ListTag(s) & |: | & s
		End If
	End ForAll
	enhLogException LIB_PREFIX, msg$
	result$ = |{}|
	Resume ExitPoint 
End Sub

