%REM
	Sub enhLogComplexElement
	Logs an element using enhanced log extended content 
	
	@param source 	Element to log 
	@param tagname	Name of element for logging 
%END REM
Private Sub enhLogComplexElement(source As Variant, tagname$) 
	On Error GoTo ErrorTrap
	
	Static recursions As Integer 
	
	' exception variables
	Dim extendedinfo List As String 
	Dim msg$
	
	Dim sourcetype$
	
	
	' BEGIN CODE 
	If (recursions > 5) Then
		enhLogWarning LIB_PREFIX, |Too many recursions, aborting|  
		GoTo ExitPoint
	End If  
	
	recursions = recursions + 1
	extendedinfo(|TagName|) = tagname$ 
	
	sourcetype$ = TypeName(source) 
	extendedinfo(|Source Type|) = sourcetype$ 
	Select Case LCase(sourcetype$) 
		Case |azlwlistcarrier|:
			enhLogComplexElement source.getContent(), tagname$
			
		Case |booleanlist|, _ 
		|currencylist|, _ 
		|datelist|, _ 
		|doublelist|, _ 
		|integerlist|, _ 
		|longlist|, _ 
		|singlelist|, _ 
		|stringlist|, _ 
		|variantlist|: 
			enhLogComplexElement source.content, tagname$

		Case Else:
			If IsScalar(source) Then
				enhLogExtendedContent ||, ||, tagname$ & |: | & CStr(source)  
			ElseIf IsList(source) Then 
				enhLogExtendedContent ||, ||, tagname$
				ForAll listElement In source 
					enhLogComplexElement listElement, HT & ListTag(listElement)   			
				End ForAll 
				enhLogExtendedContent ||, ||, ||
				
			ElseIf IsArray(source) Then
				enhLogExtendedContent ||, ||, tagname$
				ForAll arrayelement In source 
					enhLogComplexElement arrayelement, HT & HT  			
				End ForAll 
				enhLogExtendedContent ||, ||, ||
			Else
				enhLogExtendedContent ||, ||, tagname$ & |: - UNABLE TO LOG | & TypeName(source) 
			End If 
	End Select
	
	
	
	

	
ExitPoint: 
	If (recursions > 0) Then recursions = recursions - 1
	Exit Sub 
ErrorTrap:
	On Error GoTo 0
	ForAll s In extendedinfo
		If isBlankString(msg$) Then 
			msg$ = ListTag(s) & |: | & s
		Else 
			msg$ = msg$ & |, | & Chr(10) & ListTag(s) & |: | & s
		End If
	End ForAll
	enhLogException LIB_PREFIX, msg$
	Resume ExitPoint
End Sub

