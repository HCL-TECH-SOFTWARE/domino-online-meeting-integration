				If IsNumeric(source) Then 
					result$ = LCase$(result$) ' Converts Boolean True to "true" and False to "false" 
					If (source < 1) Then 
						If ("" = result$) Or (0 = StrCompare(".", Left$(result$, 1), 5)) Then result$ = "0" & result$
					End If 
