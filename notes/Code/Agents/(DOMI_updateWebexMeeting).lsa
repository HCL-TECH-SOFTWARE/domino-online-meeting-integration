</lotusscript></code><code event='initialize'><lotusscript>

Sub Initialize
	On Error Goto ErrorTrap

	Dim nodesToCopy As New StringList() 
	
	
	' BEGIN CODE 
	Call instantiateDOMIlog() ' Instantiate Logging and set options for DOMI operations

	nodesToCopy.content(|meetingID|) = 	   				|id| 
	nodesToCopy.content(|meetingTitle|) = 				|title| 
	nodesToCopy.content(|meetingURL|) = 				|webLink| 
	nodesToCopy.content(|ApptUNIDURL|) =	 			|webLink| 
	nodesToCopy.content(|meetingStartStr|) = 			|start| 
	nodesToCopy.content(|meetingEndStr|) = 				|end| 
	nodesToCopy.content(|meetingPassword|) =			|password|
	nodesToCopy.content(|meetingPhoneVideoPassword|) = 	|phoneAndVideoSystemPassword|
	nodesToCopy.content(|meetingSipAddress|) = 			|sipAddress|
	
	Call domiUpdateMeeting(nodesToCopy, DOMI_SERVICE_WEBEX) 
	
ExitPoint:
	Exit Sub		
ErrorTrap:
	On Error Goto 0
	enhLogAction ||, || 
	Resume ExitPoint 		
End Sub</lotusscript></code>
