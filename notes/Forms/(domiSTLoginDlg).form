<body><richtext>
<pardef id='1' leftmargin='1in' hide='read edit print preview previewedit notes web'><parstyle
 name='Hidden Text' incyclekey='true'><font size='8pt' color='green' name='sans-serif'/></parstyle></pardef>
<pardef id='2' leftmargin='1in' hide='notes web mobile'/>
<par def='2'><run><font size='8pt' color='green'/></run></par>
<table cellbordercolor='silver' widthtype='fixedleft' rowspacing='0.0500in'
 columnspacing='0.0500in' refwidth='4.8375in'><tablecolumn width='1.7625in'/><tablecolumn
 width='2.8750in'/>
<tablerow>
<tablecell columnspan='2' borderwidth='0px 0px 1px' bgcolor='white'>
<pardef id='5' keepwithnext='true' keeptogether='true'/>
<par def='5'><run><font size='11pt' style='bold'/>Sametime Credentials</run></par>
<par def='5'><run><font size='9pt'/>Please enter your login email and password for your Sametime Meetings server.</run></par>
<par def='5'><run><font size='9pt' style='bold' color='red'/>NOTE:</run><run
><font size='9pt' color='red'/> </run><run><font size='9pt'/>Your password will </run><run
><font size='9pt' style='bold' color='red'/>NOT</run><run><font size='9pt'/> be saved</run></par></tablecell></tablerow>
<tablerow>
<tablecell borderwidth='1px 0px' bgcolor='#efefef'>
<pardef id='6' keepwithnext='true' keeptogether='true'/>
<par def='6'><run><font size='9pt'/>Login Email</run></par></tablecell>
<tablecell borderwidth='1px 0px' bgcolor='white'>
<pardef id='7' keepwithnext='true' keeptogether='true'/>
<par def='7'><run><font size='9pt'/></run><run><font size='9pt'/><field usenotesstyle='false'
 height='0.2500in' width='2.7000in' borderstyle='none' type='text' kind='editable'
 name='loginEmail'><code event='defaultvalue'><formula>loginEmail</formula></code><code
 event='inputtranslation'><formula>@Trim(@ThisValue)</formula></code><code
 event='inputvalidation'><formula>@If("" = @ThisValue; @Failure("Login Email is required."); @Success)</formula></code></field></run><run
><font size='9pt'/></run></par></tablecell></tablerow>
<tablerow>
<tablecell borderwidth='1px 0px' bgcolor='#efefef'>
<par def='6'><run><font size='9pt'/>Login Password (not saved)</run></par></tablecell>
<tablecell borderwidth='1px 0px' bgcolor='white'>
<par def='7'><run><font size='9pt'/></run><run><font size='9pt'/><field type='password'
 usenotesstyle='false' height='0.2500in' width='2.7000in' multiline='true'
 borderstyle='none' kind='editable' seal='true' name='stPassword'><code event='defaultvalue'><formula
>""</formula></code><code event='inputtranslation'><formula>@Trim(@ThisValue)</formula></code><code
 event='inputvalidation'><formula>@If("" = @ThisValue; @Failure("Login Password is required."); @Success)</formula></code></field></run><run
><font size='9pt'/></run></par></tablecell></tablerow></table>
